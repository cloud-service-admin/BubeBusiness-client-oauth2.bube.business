    "@yudiel/react-qr-scanner": "^2.0.8",
    "antd": "^5.15.3",
    "axios": "^1.6.1",
    "country-state-city": "^3.2.1",
    "dayjs": "^1.11.10",
    "file-saver": "^2.0.5",
    "firebase": "^11.4.0",
    "html5-qrcode": "^2.3.8",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.3.1",
    "image-type": "^5.2.0",
    "iso-country-currency": "^0.7.2",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "moment-timezone": "^0.5.45",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-cookie": "^7.2.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^13.3.1",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-webcam": "^7.2.0",
