{
  "name": "oauth2-bube-business",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev --host",
    "test": "vite --mode test",
    "testdev": "vite --mode test.dev",
    "testpubic": "vite --mode test.public",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint:fix": "eslint --fix src --ext ts,tsx",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "start:test": "cp .firebasesrc.test .firebasesrc && cp firebase.test.json firebase.json && npm run serve",
    "start:main": "cp .firebasesrc.main .firebasesrc && cp firebase.main.json firebase.json && npm run serve"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@hookform/resolvers": "^3.3.2",
